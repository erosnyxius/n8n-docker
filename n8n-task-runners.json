{
  "task-runners": [
    {
      "runner-type": "javascript",
      "dir": "/opt/runners/task-runner-javascript",
      "health-check-server-port": "5681",
      "env-overrides": {
        "NODE_FUNCTION_ALLOW_BUILTIN": "*",
        "NODE_FUNCTION_ALLOW_EXTERNAL": "moment,lodash,uuid,axios"
      }
    },
    {
      "runner-type": "python",
      "dir": "/opt/runners/task-runner-python",
      "health-check-server-port": "5682",
      "env-overrides": {
        "PYTHONPATH": "/opt/runners/task-runner-python",
        "N8N_RUNNERS_STDLIB_ALLOW": "json,math,sys,datetime,os,random,re",
        "N8N_RUNNERS_EXTERNAL_ALLOW": "numpy,pandas,requests,beautifulsoup4"
      }
    }
  ]
}